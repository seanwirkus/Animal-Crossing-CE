{
  "name": "ACNH Roblox",
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "ModuleLoader": {
        "$path": "src/shared/ModuleLoader.luau"
      },
      "Logger": {
        "$path": "src/shared/Logger.luau"
      },
      "Data": {
        "$path": "src/shared/Data"
      },
      "GameTypes": {
        "$path": "src/shared/GameTypes.luau"
      },
      "ErrorHandler": {
        "$path": "src/shared/ErrorHandler.luau"
      },
      "Configs": {
        "$path": "src/shared/Configs"
      },
      "Remotes": {
        "$path": "src/shared/Remotes.luau"
      },
      "Components": {
        "$path": "src/shared/Components"
      },
      "Controllers": {
        "$path": "src/shared/Controllers"
      },
      "Assets": {
        "$path": "src/assets"
      }
    },
    "ServerScriptService": {
      "$path": "src/server"
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "$path": "src/client"
      }
    },
    "StarterGui": {
      "MainUI": {
        "$className": "ScreenGui",
        "$properties": {
          "ResetOnSpawn": false
        }
      }
    },
    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      },
      "Base": {
        "$className": "Folder"
      },
      "Baseplate": {
        "$className": "Part",
        "$properties": {
          "Anchored": true,
          "Color": [0.2, 0.8, 0.2],
          "Locked": true,
          "Position": [0, -10, 0],
          "Size": [512, 20, 512]
        }
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [0.5, 0.8, 1.0],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}